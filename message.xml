<tryton>
    <data group="1">
      <record model="ir.message" id="no_numbers">
          <field name="text">No numbers found in string "%(number)s".</field>
      </record>
      <record model="ir.message" id="serial_number">
          <field name="text">Move "%(move)s" can not be done as its product "%(product)s" is marked as serial number and its quantity is different than 1.</field>
      </record>
</data>
</tryton>
